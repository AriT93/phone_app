.span9.agent
  h1 Agent Information
  ul.ui-widget
    li.ui-widget-header
      .span2.alpha
        label(for = "agentPhone") Phone
      .prefix_1.span2
        label(for = "zip") Zip Code
      .prefix_1.span2.omega
        label(for = "distance") Distance
    li.ui-widget-content
      .span2.alpha
        input(id = "agentPhone", name = "agentPhone", type = "text", value = "(309) 123-4958" )
      .prefix_1.span2
        input(id = "zip", name = "zip", type = "text")
      .prefix_1.span2.omega
        input(id = "distance", name = "distance", value = "30", type = "text")
        input(id = "city", name = "city", type = "hidden")
        input(id = "state", name = "state", type = "hidden")
        input(id = "latitude", name = "latitude", type = "hidden")
        input(id = "longitude", name = "longitude", type = "hidden")
.span15#calls
  h1 Calls
  ul#callList.ui-widget
    li.ui-widget-header
      .span2.alpha
        = "Name"
      .span2
        = "Phone"
      .span2
        = "City"
      .span2
        = "State"
      .span2
        = "Zip Code"
      .span2
        Time
      .span1.omega
      - each call in calls
        li.ui-widget-content.call(id = "#{call.tn}", createdOn = "#{call.createdOn}",lat = "#{call.latitude}", long = "#{call.longitude}", state = "#{call.state}")
          .span2.alpha
            != call.name
          .span2
            != call.tn
          .span2
            != call.city
          .span2
            != call.state
          .span2
            != call.zip
          .span2(title = "timeElapsed")
            = 0
          .span1.omega
            button( rel = "##{call.tn}_ov", onclick = "takeCall(#{call.tn});")
              img.phone_icon(src = '/img/phone.png')
#calls_ov
  - each call in calls
    .simple_overlay(id = "#{call.tn}_ov")
      img(src = "/img/phone.png")
      .span12#calls
        ul
          li
            .span5.alpha.ov_top
              = call.name
            .span5.omega.ov_top
              = call.tn
          li
            .span2.alpha
              = call.city
            .span2
              = call.state
            .span2.omega
              = call.zip
