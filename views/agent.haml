.grid_10#calls
  %h1 Calls
  %ul#callList
    %li
      .grid_2.alpha
        = "Name"
      .grid_2
        = "Phone"
      .grid_2
        = "City, State"
      .grid_2
        = "Zip Code"
      .grid_1.omega
    - @coll.find("status" => {"$ne" => "called"}).each do |row|
      %li.call{:id => "#{row['tn']}",:lat => "#{row['latitude']}", :long => "#{row['longitude']}"}
        .grid_2.alpha
          = row['name'] || "-"
        .grid_2
          = row['tn'] || "-"
        .grid_2
          = "#{row['city']}, #{row['state']}"
        .grid_2
          = row['zip'] || "-"
        .grid_1.omega
          %button{ :rel => "##{row['tn']}_ov", :onclick => "takeCall(#{row['tn']} );"}
            %img.phone_icon{:src => '/img/phone.png'}
.grid_5#messages
  %h1 Messages3
#calls_ov
  - @coll.find().each do |row|
    .simple_overlay{:id => "#{row['tn']}_ov"}
      %img{:src => '/img/phone.png'}
      .grid_12#calls
        %ul
          %li
            .grid_2.alpha
              = "#{row['name']}"
            .grid_2.omega
              = "#{row['tn']}"
          %li
            .grid_2.alpha
              = "#{row['city']}"
            .grid_2
              = "#{row['state']}"
            .grid_2.omega
              = "#{row['zip']}"
